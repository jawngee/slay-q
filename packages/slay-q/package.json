{
  "name": "@slay-pics/slay-q",
  "version": "0.2.1",
  "description": "Slay Q is a queue and background job management system inspired by Inngest.  It's in production use on https://slay.pics for managing all of our media processing, notifications and other services.",
  "repository": "SlayPics/SlayUtils",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "keywords": [
    "slayq",
    "slay-q",
    "inngest",
    "qstash",
    "job queue",
    "background tasks",
    "queue",
    "step function",
    "job scheduler",
    "task manager",
    "job processing",
    "workflow"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "watch": "node --watch-path=src dist/index.mjs",
    "dev": "vitest dev",
    "play": "jiti playground",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src test",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test -w",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@types/ms": "^0.7.34",
    "@types/node": "^20.10.5",
    "@vitest/coverage-v8": "^1.1.0",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "eslint-config-unjs": "^0.2.1",
    "jiti": "^1.21.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "@slay-pics/date-utils": "workspace:*",
    "@slay-pics/signature": "workspace:*",
    "cron-parser": "^4.9.0",
    "ms": "^2.1.3",
    "short-uuid": "^4.2.2",
    "zod": "^3.22.4"
  },
  "packageManager": "pnpm@8.12.1"
}
